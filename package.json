  "license": "ISC",
  "dependencies": {
    "@ionic/app-scripts": "^3.2.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cloudinary": "^1.28.1",
    "concurrently": "^3.6.1",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
    "mongoose": "^5.13.14",
    "multer": "^1.4.4",
    "multer-storage-cloudinary": "^2.2.1",
    "npm": "^8.11.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  }
